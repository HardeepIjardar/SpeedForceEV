{
  "name": "speedforceev-server",
  "version": "1.0.0",
  "description": "Server for SpeedForceEV Live Fleet Stats API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
